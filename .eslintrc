{
	"parser": "babel-eslint",
	"parserOptions": {
		"parser": "babel-eslint",
		"ecmaVersion": 2018,
		"sourceType": "module",
		"allowImportExportEverywhere": true
	},
	"plugins": [
		"jsdoc"
	],
	"extends": "eslint:recommended",
	"env": {
		"amd": true,
		"browser": true,
		"jquery": true,
		"node": true,
		"es6": true,
		"worker": true
	},
	"globals": {"chrome": true},
	"rules": {
		"no-inner-declarations": 0,
		"no-shadow": 1,
		"no-redeclare": 1,
		"no-mixed-spaces-and-tabs": 2,
		"no-unused-vars": 1,
		"quotes": [1, "double"],
		"comma-dangle": 1,
		"no-console": 0,
		"no-extra-semi": 1,
		"no-extra-parens": 0,
		"semi-spacing": 1,
		"require-jsdoc": [1, {
			"require": {
				"FunctionDeclaration": true,
				"MethodDefinition": true,
				"ClassDeclaration": true,
				"ArrowFunctionExpression": false,
				"FunctionExpression": false
			}
		}],
		"jsdoc/check-param-names": 1,
		"jsdoc/check-tag-names": 1,
		"jsdoc/check-types": 1,
		"jsdoc/newline-after-description": 0,
		"jsdoc/no-undefined-types": 1,
		"jsdoc/require-description": 0,
		"jsdoc/require-description-complete-sentence": 0,
		"jsdoc/require-example": 0,
		"jsdoc/require-hyphen-before-param-description": 0,
		"jsdoc/require-param": 1,
		"jsdoc/require-param-description": 1,
		"jsdoc/require-param-name": 1,
		"jsdoc/require-param-type": 1,
		"jsdoc/require-returns-description": 0,
		"jsdoc/require-returns-type": 1,
		"jsdoc/valid-types": 1,
		"no-bitwise": 1,
		"camelcase": 2,
		"curly": 0,
		"eqeqeq": 2,
		"guard-for-in": 0,
		"no-extend-native": 2,
		"wrap-iife": [2, "any"],
		"indent": [1, "tab", {"SwitchCase": 1}],
		"no-use-before-define": [2, {"functions": false}],
		"new-cap": 1,
		"no-caller": 2,
		"no-empty": 2,
		"no-irregular-whitespace": 2,
		"no-new": 2,
		"no-plusplus": 1,
		"no-undef": 2,
		"strict": 2,
		"max-params": [2, 11],
		"max-depth": [1, 2],
		"max-statements": [0, 2],
		"complexity": [0, 4],
		"max-len": [2, {
			"code": 120,
			"ignoreComments": true
		}],
		"semi": 1,
		"no-cond-assign": 1,
		"no-debugger": 1,
		"no-eq-null": 2,
		"no-eval": 1,
		"no-unused-expressions": 1,
		"block-scoped-var": 1,
		"no-iterator": 1,
		"linebreak-style": 0,
		"comma-style": [2, "last"],
		"no-loop-func": 1,
		"no-multi-str": 1,
		"no-multi-spaces": 1,
		"valid-typeof": 1,
		"no-proto": 1,
		"no-script-url": 1,
		"dot-notation": 2,
		"no-new-func": 1,
		"no-new-wrappers": 1,
		"no-invalid-this": 0
	}
}